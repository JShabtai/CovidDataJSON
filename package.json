{
  "name": "data-preprocessor",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Josh Shabtai",
  "license": "MIT",
  "scripts": {
    "lint": "eslint",
    "update:git": "cd COVID-19 && git pull",
    "build": "tsc",
    "build:watch": "tsc -w",
    "run": "node build/index.js",
    "generate:all": "npm run run -- --minCases 0 >./data/covid.all.json",
    "generate:5": "npm run run -- --minCases 5 >./data/covid.5.json",
    "generate:100": "npm run run -- --minCases 100 >./data/covid.100.json",
    "generate:all:min": "npm run run -- --minCases 0 --skipMissingDates >./data/covid.all.min.json",
    "generate:5:min": "npm run run -- --minCases 5 --skipMissingDates >./data/covid.5.min.json",
    "generate:100:min": "npm run run -- --minCases 100 --skipMissingDates >./data/covid.100.min.json",
    "gzip": "gzip -kf data/*.json",
    "update": "mkdir data; npm-run-all build generate:all generate:5 generate:100 generate:all:min generate:5:min generate:100:min gzip"
  },
  "dependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/node": "^14.0.6",
    "@vanillaes/csv": "^2.1.1",
    "command-line-args": "^5.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "3.7.5"
  }
}
